  "dependencies": {
    "kronos-step": "5.1.3"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.1.5",
    "istanbul": "0.4.2",
    "jsdoc": "3.4.0",
    "kronos-service-manager": "3.2.14",
    "kronos-test-step": "3.1.0",
    "mocha": "2.4.5",
    "semantic-release": "6.2.1",
    "stream-equal": "^0.1.8",
    "tmp": "0.0.28"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
